<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:infra="clr-namespace:Calculadora.Infrastructure;assembly=Calculadora"
             x:Class="Calculadora.App">
	<Application.Resources>
        <ResourceDictionary>
            <!-- Locator -->
            <infra:InstanceLocator x:Key="Locator"></infra:InstanceLocator>
            <!--Colors-->
            <Color x:Key="MainColor">#313E40</Color>
            <Color x:Key="FontColor">#152026</Color>
            <Color x:Key="AccentColor1">#BEBFB4</Color>
            <Color x:Key="AccentColor2">#73736C</Color>
            <Color x:Key="BackgroundColor">#F2F2F2</Color>
        </ResourceDictionary>
    </Application.Resources>
</Application>